        <Button Content="Volver" HorizontalAlignment="Left" Margin="71,364,0,0" VerticalAlignment="Top" Click="Button_Click" Height="34" Width="101"/>
        <Button Content="Editar" HorizontalAlignment="Left" Margin="628,364,0,0" VerticalAlignment="Top" Click="Button_Click" Height="34" Width="101"/>

    </Grid>
